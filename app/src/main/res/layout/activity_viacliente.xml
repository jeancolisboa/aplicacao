<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ViaclienteActivity">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="375dp"
        android:text="Deseja imprimir\n Via do Cliente?"
        android:textColor="@color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:gravity="center"
        android:padding="60dp"
        android:background="@drawable/bordas_arrendondadas"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.32"/>

    <Button
        android:id="@+id/negativo"
        android:layout_width="205.6dp"
        android:layout_height="48dp"
        android:background="@drawable/estilocancelar"
        android:text="@string/n_o"
        android:textSize="20sp"
        android:textAlignment="center"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.9915555" />

    <Button
        android:id="@+id/positivo"
        android:layout_width="205.6dp"
        android:layout_height="48dp"
        android:text="@string/sim"
        android:textSize="20sp"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:background="@drawable/estilocancelar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.991555" />

    <TextView
        android:layout_width="205.6dp"
        android:layout_height="8dp"
        android:background="@drawable/estiloavancar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.9999"/>

    <TextView
        android:layout_width="205.6dp"
        android:layout_height="8dp"
        android:background="@drawable/estilonegar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.9999"/>


</androidx.constraintlayout.widget.ConstraintLayout>